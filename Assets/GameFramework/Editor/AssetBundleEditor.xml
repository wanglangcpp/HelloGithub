<?xml version="1.0" encoding="UTF-8"?>
<UnityGameFramework>
  <AssetBundleEditor>
    <Setting>
      <SourceAssetRootPath>Assets</SourceAssetRootPath>
      <SourceAssetSearchPaths>
        <SourceAssetSearchPath RelativePath="Main" />
        <SourceAssetSearchPath RelativePath="ToLua" />
        <SourceAssetSearchPath RelativePath="NGUI/Resources/Shaders" />
      </SourceAssetSearchPaths>
      <SourceAssetUnionTypeFilter>t:Scene t:Prefab t:TextAsset t:ScriptableObject t:AudioClip t:Font t:Shader</SourceAssetUnionTypeFilter>
      <SourceAssetUnionLabelFilter>l:AssetBundleMust l:LuaScript</SourceAssetUnionLabelFilter>
      <SourceAssetExceptTypeFilter>t:Script</SourceAssetExceptTypeFilter>
      <SourceAssetExceptLabelFilter>l:AssetBundleIgnore</SourceAssetExceptLabelFilter>
      <AssetSorter>Name</AssetSorter>
    </Setting>
  </AssetBundleEditor>
</UnityGameFramework>